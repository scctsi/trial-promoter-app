.facebook-ad.newsfeed
  %article
    %header
      .controls
        %button.facebook-button
          %i.thumbs.up.icon
          Like Page
      .profile
        %img.avatar{ src: "data:image/svg+xml;base64," }
        .profile-title
          %a.name
            Your Page Here
          %a.sponsored
            Sponsored
    %p.message
      = text
    %main
      .image
        %img{ src: image_url }
      .details
        %p.headline
          = headline
        %p.description
          = link_description
        %footer
          .controls
            %button.facebook-button
              = call_to_action
          %p.caption
            www.test.com
  %footer
    .stats
      %span.reactions
        %i.small.circular.inverted.blue.thumbs.up.icon
      %span.count 20
      %span.social
        %span.comments
          500 Comments
        %span.shares
          100 Shares
    %hr
    .actions
      %span.like
        %i.thumbs.up.icon
        Like
      %span.comment
        %i.comment.icon
        Comment
      %span.share
        %i.share.icon
        Share
